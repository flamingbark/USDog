# BSC Deployment Configuration
# Copy this file to .env and fill in your values

# Private key for deployment (without 0x prefix)

# DAO Safe Address (will receive admin rights after deployment)
# DAO_SAFE_ADDRESS=0xD1F1CC33b06cF00884E4F74A2Bc8f2B11863A64B

# BSC RPC URLs (optional - defaults are provided)
BSC_RPC_URL=https://bsc-dataseed1.binance.org/
BSC_TESTNET_RPC_URL=https://data-seed-prebsc-1-s1.binance.org:8545/
# BSC WebSocket URL (optional - required for live monitoring)
BSC_WSS_URL=wss://bsc-ws-node.nariox.org:443

# DOGE/SHIB liquidation bot contract (deployed address)
DOGE_SHIB_BOT_CONTRACT=your_doge_shib_bot_address_here

# Dry run mode (true/false). When true, bot will not send transactions and can run without DOGE_SHIB_BOT_CONTRACT
DRY_RUN=true

# BSC API Keys for contract verification